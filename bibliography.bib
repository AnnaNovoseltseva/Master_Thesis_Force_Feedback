@ARTICLE{Susil1999,
  author = {Robert C. Susil and James H. Anderson and Russell H. Taylor},
  title = {A Single Image Registration Method for CT Guided Interventions},
  journal = {Med Image Comput Comput Assist Interv Int Conf Med Image Comput Comput
	Assist Interv},
  year = {1999},
  volume = {1679},
  pages = {798--808},
  month = {September},
  abstract = {Minimally invasive image guided interventions are an attractive option
	for localized therapy delivery and diagnostic biopsy. We have developed
	a method for CT guided needle placement, based upon the Brown-Roberts-Wells
	frame, which requires no immobilization or fiducial implantation.
	A localization module, placed on a needle holding robotic end effector,
	allows for localization of the effector in the image space using
	a single CT image. In a theoretical analysis, we show that this registration
	method has attractive sensitivity and error attenuation properties.
	Experimentally, the average error in needle tip location over 63
	trials was 470um; 95% of the errors were below 1.0 mm. This method
	is a fast, accurate, and easily implemented registration method for
	cross sectional image guided stereotaxis.}
}

