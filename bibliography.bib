@ARTICLE{Beyersdorff2005,
  author = {Dirk Beyersdorff and Axel Winkel and Bernd Hamm and Severin Lenk
	and Stefan A Loening and Matthias Taupitz},
  title = {{MR imaging-guided prostate biopsy with a closed MR unit at 1.5 T:
	initial results.}},
  journal = {Radiology},
  year = {2005},
  volume = {234},
  pages = {576--581},
  number = {2},
  month = {Feb},
  abstract = {The authors evaluated a magnetic resonance (MR) imaging-compatible
	biopsy device comprising a needle guide that can be visualized with
	MR imaging and manipulated mechanically from outside the MR unit.
	With approval from the local ethics committee and patient consent,
	this device was tested in 12 patients by using a closed 1.5-T MR
	unit and a body phased-array coil. Patients had elevated prostate-specific
